<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DicasDeTurismoActivity">

    <TextView
            android:text="Dicas de turismo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView48" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            android:textAlignment="center" android:textColor="#161313" android:textSize="24sp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.039"/>
    <ImageView
            android:layout_width="90dp"
            android:layout_height="90dp" app:srcCompat="@drawable/tur" tools:layout_editor_absoluteY="3dp"
            android:id="@+id/imageView12" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.026"/>
    <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp" app:srcCompat="@drawable/praia"
            android:id="@+id/BtnPraiasImg" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView12" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.057"
            app:layout_constraintVertical_bias="0.066"/>
    <Button
            android:text="Praias"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/BtnPraia" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/BtnPraiasImg"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView48"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.107"/>
    <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp" app:srcCompat="@drawable/parques"
            android:id="@+id/BtnParquesImg" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnPraia"
            app:layout_constraintHorizontal_bias="0.975" app:layout_constraintVertical_bias="0.05"/>
    <Button
            android:text="Parques"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/BtnParques" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnPraia" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/BtnParquesImg"
            android:textAlignment="center" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.05"/>
    <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp" app:srcCompat="@drawable/museu"
            android:id="@+id/BtnMuseuImg" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnParques"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.024" app:layout_constraintVertical_bias="0.097"/>
    <Button
            android:text="Museu"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/BtnMuseu" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/BtnMuseuImg" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnParquesImg"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.097"/>
    <Button
            android:text="Voltar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            android:id="@+id/BtnVoltarDeDicasDeTurismo" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/BtnMuseu"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.967"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="70dp" app:srcCompat="@drawable/beithbrasil"
            android:id="@+id/imageView21" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltarDeDicasDeTurismo" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/BtnMuseu" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.981"/>
</androidx.constraintlayout.widget.ConstraintLayout>