<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AppRecomendaveisActivity">

    <TextView
            android:text="Aplicativo recomendaveis"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:textColor="#161313" android:textAlignment="center"
            android:textSize="24sp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.011"/>
    <Button
            android:text="Voltar"
            android:layout_width="313dp"
            android:layout_height="45dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="16dp" android:id="@+id/BtnVoltardeApps" android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.914"/>
    <ImageView
            android:layout_width="75dp"
            android:layout_height="46dp" app:srcCompat="@drawable/voltar"
            android:id="@+id/BtnVoltardeAppsImg" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintEnd_toStartOf="@+id/BtnVoltardeApps"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.987"/>

    <ScrollView
            android:layout_width="382dp"
            android:layout_height="610dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnVoltardeApps"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintVertical_bias="0.461">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/waze" />

            <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Wase"
                    android:textAlignment="center"
                    android:textColor="#161313"
                    android:textSize="18sp" />

            <TextView
                    android:id="@+id/textView194"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Um dos melhores programas de GPS,  criando em Israel, te ajuda a chegar a qualaquer ponto do pais sem problemas. " />

            <TextView
                    android:id="@+id/textView198"
                    android:layout_width="match_parent"
                    android:layout_height="60dp" />

            <ImageView
                    android:id="@+id/imageView80"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/moovit" />

            <TextView
                    android:id="@+id/textView200"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Moovit"
                    android:textAlignment="center"
                    android:textColor="#161313"
                    android:textSize="18sp" />

            <TextView
                    android:id="@+id/textView201"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Moovit e' um app cridao para lhe dar um conforto para uso de transporte publico " />

            <TextView
                    android:id="@+id/textView202"
                    android:layout_width="match_parent"
                    android:layout_height="60dp" />

            <ImageView
                    android:id="@+id/imageView81"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/pango" />

            <TextView
                    android:id="@+id/textView216"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Pango"
                    android:textAlignment="center"
                    android:textColor="#161313"
                    android:textSize="18sp" />

            <TextView
                    android:id="@+id/textView217"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Pango, substiu no vamosa folhinha de zuna azul. Com esse app, basta clicar ." />

            <TextView
                    android:id="@+id/textView218"
                    android:layout_width="match_parent"
                    android:layout_height="60dp" />

            <ImageView
                    android:id="@+id/imageView104"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/gettaxi" />

            <TextView
                    android:id="@+id/textView219"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Get"
                    android:textAlignment="center"
                    android:textColor="#161313"
                    android:textSize="18sp" />

            <TextView
                    android:id="@+id/textView220"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Get, chamar taxi com a mao e' coisa do passado, agora o negocio e' clicar e pronto, em pouco tempo tera um taxi no seu endereco " />

            <TextView
                    android:id="@+id/textView221"
                    android:layout_width="match_parent"
                    android:layout_height="60dp" />

            <ImageView
                    android:id="@+id/imageView106"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/myvisit" />

            <TextView
                    android:id="@+id/textView222"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="MyVisit"
                    android:textAlignment="center"
                    android:textColor="#161313"
                    android:textSize="18sp" />

            <TextView
                    android:id="@+id/textView223"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="MyVisit, foi criado para diminuir com as filas nos principais servicos publico. Marque o dia e a hora da sua visita e evite filas " />

            <TextView
                    android:id="@+id/textView224"
                    android:layout_width="match_parent"
                    android:layout_height="60dp" />

            <ImageView
                    android:id="@+id/imageView107"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/red" />

            <TextView
                    android:id="@+id/textView225"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Red Alert"
                    android:textAlignment="center"
                    android:textColor="#161313"
                    android:textSize="18sp" />

        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>