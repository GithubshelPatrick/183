<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AnunciarClassificadosActivity">

    <TextView
            android:text="Anunciar nos classificados "
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView83"
            android:textColor="#181313"
            android:textSize="24sp" android:textAlignment="center"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="7dp" android:layout_marginEnd="7dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Tipo : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView85"
            android:textColor="#161313"
            app:layout_constraintStart_toStartOf="@+id/editText2"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView5" android:layout_marginStart="49dp"/>
    <TextView
            android:text="Descrição  : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView86" android:textColor="#161313"
            android:layout_marginTop="142dp"
            app:layout_constraintEnd_toStartOf="@+id/editText2" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="3dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText2"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView86"
            app:layout_constraintStart_toEndOf="@+id/textView86" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText4"
            android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/textView86" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText5"
            android:layout_marginTop="3dp"
            app:layout_constraintTop_toBottomOf="@+id/editText4" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="9dp" android:layout_marginEnd="9dp"/>
    <TextView
            android:text="Fotos : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView87" android:textColor="#161313"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            android:layout_marginStart="8dp" app:layout_constraintTop_toBottomOf="@+id/editText5"/>
    <Button
            android:text="Ver anúncio"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/BtnOlharAnuncio"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="16dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="8dp"/>
    <TextView
            android:text="Seu nome : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView116"
            android:textColor="#161313"
            app:layout_constraintEnd_toStartOf="@+id/editText7" android:layout_marginBottom="188dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="8dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="47dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText7"
            android:layout_marginTop="74dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView7" app:layout_constraintStart_toEndOf="@+id/textView116"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="75dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnOlharAnuncio" android:layout_marginEnd="8dp"
    />
    <TextView
            android:text="Para ser contactado :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView117"
            android:textColor="#161313"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="9dp" app:layout_constraintBaseline_toBaselineOf="@+id/editText8"
            app:layout_constraintEnd_toStartOf="@+id/editText8" android:layout_marginEnd="8dp"
    />
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText8"
            android:hint="telefone ou e-mail"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText7" app:layout_constraintStart_toEndOf="@+id/textView117"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="25dp"
            app:layout_constraintBottom_toTopOf="@+id/BtnOlharAnuncio" android:layout_marginEnd="11dp"
    />
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/foto"
            android:id="@+id/imageView7"
            app:layout_constraintEnd_toStartOf="@+id/imageView9" app:layout_constraintTop_toTopOf="@+id/imageView9"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="9dp" app:layout_constraintBottom_toBottomOf="@+id/imageView9"
            android:layout_marginEnd="21dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="120dp" app:srcCompat="@drawable/foto"
            android:id="@+id/imageView9"
            android:layout_marginTop="93dp"
            app:layout_constraintStart_toEndOf="@+id/imageView7" app:layout_constraintEnd_toStartOf="@+id/imageView10"
            android:layout_marginBottom="94dp" app:layout_constraintBottom_toTopOf="@+id/textView116"
            app:layout_constraintTop_toTopOf="@+id/textView6" android:layout_marginEnd="11dp"
    />
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/foto"
            android:id="@+id/imageView10"
            app:layout_constraintStart_toEndOf="@+id/imageView9" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView9" app:layout_constraintBottom_toBottomOf="@+id/imageView9"
            android:layout_marginEnd="10dp"/>
    <ImageView
            android:layout_width="62dp"
            android:layout_height="48dp" app:srcCompat="@drawable/camera"
            android:id="@+id/ImgCamera"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintTop_toTopOf="@+id/textView87" android:layout_marginTop="19dp"
            android:layout_marginBottom="18dp" app:layout_constraintBottom_toTopOf="@+id/imageView9"/>
    <TextView
            android:text="Autos "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textColor="#161313"
            android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@+id/textView83" app:layout_constraintStart_toEndOf="@+id/textView85"/>
    <TextView
            android:text="Abrir camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/textView87" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.398" android:textColor="#161313"
            app:layout_constraintTop_toBottomOf="@+id/editText5"/>

</androidx.constraintlayout.widget.ConstraintLayout>